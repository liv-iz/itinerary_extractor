{
  "manifest_version": 3,
  "name": "Itinerary Location Extractor",
  "version": "1.0",
  "description": "Extracts locations from an itinerary on a webpage using Gemini.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html"
}
